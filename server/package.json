{
  "name": "@eboggs302/scavenger_hunt.server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development tsx watch ./server_gql.ts",
    "start-server": "NODE_ENV=production node --experimental-modules --es-module-specifier-resolution=node ./dist/server_gql.js",
    "tsc-check": "tsc --noEmit",
    "codegen": "graphql-codegen --config codegen.ts",
    "compile": "rm -rf ./dist && npx tsc && cp -R node_modules ./dist/node_modules && cp -R graphql ./dist/graphql",
    "test": "vitest"
  },
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "@graphql-tools/graphql-file-loader": "^8.0.11",
    "@graphql-tools/load": "^8.0.12",
    "@graphql-tools/load-files": "^7.0.0",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-jwt": "^8.5.1",
    "express-session": "^1.18.1",
    "graphql": "^16.11.0",
    "graphql-mongodb-subscriptions": "^1.0.1",
    "graphql-subscriptions": "^3.0.0",
    "graphql-ws": "^6.0.5",
    "gridfs-stream": "^1.1.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.12.0",
    "mongoose": "^8.9.5",
    "parse-duration": "^1.1.1",
    "stripe": "^18.1.1",
    "twilio": "^5.5.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.6",
    "@graphql-codegen/fragment-matcher": "^5.1.0",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-mongodb": "^3.0.1",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-codegen/typescript-resolvers": "^4.5.1",
    "@swc/core": "^1.4.6",
    "@types/bcryptjs": "^2.4.2",
    "@types/bson": "^4.2.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-jwt": "^7.4.2",
    "@types/express-session": "^1.17.7",
    "@types/graphql": "^14.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^22.10.1",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^2.1.8",
    "bcryptjs": "^2.4.3",
    "supertest": "^7.0.0",
    "tsx": "^4.19.4",
    "typescript": "^5.4.2",
    "vite": "^7.0.0",
    "vitest": "^3.2.4"
  }
}
