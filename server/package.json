{
  "name": "@eboggs302/scavenger_hunt.server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start-rest": "npx nodemon server_rest.ts",
    "start-gql": "npx nodemon server_gql.ts",
    "start-server": "yarn compile && node --experimental-modules --es-module-specifier-resolution=node ./tsdist/server_gql.js",
    "tsc-check": "tsc --noEmit",
    "codegen": "graphql-codegen --config codegen.ts",
    "compile": "npx tsc"
  },
  "dependencies": {
    "@apollo/server": "^4.5.0",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "express-session": "^1.17.3",
    "graphql": "^16.6.0",
    "gridfs-stream": "^1.1.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.13.20",
    "twilio": "^4.9.0",
    "typescript": "^5.0.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.2.2",
    "@graphql-codegen/fragment-matcher": "^4.0.1",
    "@graphql-codegen/introspection": "3.0.1",
    "@graphql-codegen/typescript": "3.0.2",
    "@graphql-codegen/typescript-mongodb": "2.4.6",
    "@graphql-codegen/typescript-operations": "^3.0.2",
    "@graphql-codegen/typescript-resolvers": "3.1.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/bson": "^4.2.0",
    "@types/express": "^4.17.17",
    "@types/express-jwt": "^7.4.2",
    "@types/express-session": "^1.17.7",
    "@types/graphql": "^14.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.5",
    "bcryptjs": "^2.4.3",
    "graphql-import-node": "^0.0.5",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1"
  }
}
