{
  "name": "@eboggs302/scavenger_hunt.server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn compile && npx nodemon ./dist/server_gql.js",
    "start-server": "node --experimental-modules --es-module-specifier-resolution=node ./dist/server_gql.js",
    "tsc-check": "npx tsc --noEmit",
    "codegen": "graphql-codegen --config codegen.ts",
    "compile": "rm -rf ./dist && npx tsc && cp -R node_modules ./dist/node_modules && cp -R graphql ./dist/graphql"
  },
  "dependencies": {
    "@apollo/server": "^4.9.3",
    "apollo-server-express": "^3.13.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-jwt": "^8.4.1",
    "express-session": "^1.17.3",
    "graphql": "^16.8.1",
    "gridfs-stream": "^1.1.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^8.2.1",
    "parse-duration": "^1.1.0",
    "twilio": "^5.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/fragment-matcher": "^5.0.2",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-mongodb": "3.0.0",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@graphql-codegen/typescript-resolvers": "^4.0.6",
    "@swc/core": "^1.4.6",
    "@types/bcryptjs": "^2.4.2",
    "@types/bson": "^4.2.0",
    "@types/express": "^4.17.21",
    "@types/express-jwt": "^7.4.2",
    "@types/express-session": "^1.17.7",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^20.12.2",
    "@types/supertest": "^6.0.2",
    "bcryptjs": "^2.4.3",
    "graphql-import-node": "^0.0.5",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  }
}
