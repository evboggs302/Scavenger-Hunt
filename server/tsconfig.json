{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "../tsdist/server",
    "allowJs": false,
    "noEmit": true,
    "strict": true,
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "@gql/*": ["graphql/*"],
      "@models/*": ["models/*"]
    },
    "typeRoots": ["types/**/*", "node_modules/@types"]
  },
  "exclude": ["node_modules"]
}
