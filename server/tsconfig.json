{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "baseUrl": "./",
    "esModuleInterop": true,
    "types": ["node", "jest"],
    "typeRoots": ["node_modules/@types"],
    "paths": {
      "@/*": ["./*"],
      "@generated/*": ["./generated/*"],
      "@models/*": ["./models/*"],
      "@resolvers/*": ["./resolvers/*"],
      "@utils/*": ["./utils/*"]
    }
  },
  "exclude": ["controllers/", "server_rest.ts"]
}
